{
	"name": "copy",
	"properties": {
		"activities": [
			{
				"name": "convert2paraquet",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "windowStartTime"
					},
					{
						"name": "Source",
						"value": "@{'<folderName>'}/"
					},
					{
						"name": "Destination",
						"value": "staging/test"
					}
				],
				"typeProperties": {
					"source": {
						"type": "BlobSource",
						"recursive": true
					},
					"sink": {
						"type": "AzureDataLakeStoreSink",
						"copyBehavior": "MergeFiles"
					},
					"enableStaging": false,
					"dataIntegrationUnits": 0
				},
				"inputs": [
					{
						"referenceName": "incomingData",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "staging",
						"type": "DatasetReference"
					}
				]
			}
		]
	}
}